@charset "utf-8";

body { background: white; margin: 0px; padding: 0px; color: rgb(74, 74, 74); font-family: helvetica, arial, sans-serif; font-size: 14px; line-height: 1.6; }

table { font-size: 14px; line-height: 1.6; }

table th, table td { padding: 4px 5px; }

table th { color: rgb(46, 122, 209); border-bottom: 1px solid rgb(219, 220, 222); font-size: 14px; }

h1 { font-size: 1.8em; font-weight: normal; margin: 20px 0px 10px; }

.clear { clear: both; }

.center { text-align: center; }

.left { text-align: left; }

.right { text-align: right; }

.logo { max-width: 850px; }

.container { width: 960px; max-width: 100%; margin: 0px auto; padding: 20px; box-sizing: border-box; }

.container.container__narrow { width: 640px; }

.table-container { width: 100%; }

#subscriber_info { border: 1px solid rgb(219, 220, 222); padding: 20px; background-color: rgb(245, 245, 245); margin-top: 0px; margin-bottom: 25px; text-align: left; display: flex; flex-flow: row wrap; }

#subscriber_info .subscriber_col { width: 50%; }

#subscriber_info .smalllink { font-size: 11px; overflow-wrap: break-word; }

#footer { clear: both; text-align: center; margin: 10px auto; }

#footer p { font-size: 12px; margin: 0px; }

.button { box-sizing: border-box; display: inline-block; padding: 9px 25px; background-color: rgb(171, 171, 171); border-radius: 4px; border: 1px solid transparent; font-size: 16px; font-weight: normal; text-decoration: none; cursor: pointer; transition: opacity 0.2s ease 0s, border 0.2s ease 0s, background-color 0.2s ease 0s; color: rgb(255, 255, 255) !important; }

.button:hover { background: rgb(146, 146, 146); }

.button a { text-decoration: none; white-space: nowrap; color: rgb(255, 255, 255) !important; }

.button.blue-bg { background-color: rgb(46, 122, 209); }

.button.blue-bg:hover, .button.blue-bg:focus { background-color: rgb(7, 78, 159); }

.button.x-large { font-size: 21px; padding: 16px 25px; }

.button.large { font-size: 18px; padding: 14px 25px; }

.button.small { font-size: 15px; padding: 7px 20px; }

.button.tiny { font-size: 13px; padding: 5px 20px; }

@media (max-width: 640px) {
  #subscriber_info .subscriber_col { width: 100%; }
  #subscriber_info .subscriber_col div { margin-bottom: 5px; }
  .save-changes input[type="submit"] { display: block; width: 100%; }
  .table-container { overflow-y: hidden; }
}
